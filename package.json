{
  "name": "ministry-of-finance-v1-snow-external-portal-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:proxy": "node server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:proxy\"",
    "build": "tsc -b && vite build",
    "start": "NODE_ENV=production node server.js",
    "start:windows": "set NODE_ENV=production && node server.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "heroku-postbuild": "npm run build",
    "docker:build": "docker build -t servicenow-portal .",
    "docker:run": "docker run -p 3001:3001 servicenow-portal",
    "docker:compose": "docker-compose up --build"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@shadcn/ui": "^0.0.4",
    "@types/react-datepicker": "^6.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "http-proxy-middleware": "^2.0.9",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.526.0",
    "node-fetch": "^3.3.2",
    "react": "^19.1.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.1",
    "react-router-dom": "^7.7.1",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
